  </info>
  <notepads>
  </notepads>
  <order>
    <hop>
      <from>files/products.csv</from>
      <to>(:Product)</to>
      <enabled>Y</enabled>
    </hop>
  </order>
  <transform>
    <name>(:Product)</name>
    <type>Neo4jCypherOutput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>neo4j</connection>
    <cypher>UNWIND $rows AS row
MERGE(p:Product { productID : row.productID })
SET
  p.productName = row.productName,
  p.quantityPerUnit = row.quantityPerUnit,
  p.unitPrice = row.unitPrice,
  p.unitsInStock = row.unitsInStock,
  p.unitsOnOrder = row.unitsOnOrder,
  p.reorderLevel = row.reorderLevel,
  p.discontinued = row.discontinued

WITH p, row
MATCH(c:Category { categoryID : row.categoryID })
MERGE(p)-[:HAS_CATEGORY]->(c)

WITH p, row
MATCH(s:Supplier { supplierID : row.supplierID })
MERGE(p)-[:HAS_SUPPLIER]->(s)

</cypher>
    <batch_size>1000</batch_size>
    <read_only>N</read_only>
    <nr_retries_on_error/>
    <retry>Y</retry>
    <cypher_from_field>N</cypher_from_field>
    <cypher_field/>
    <unwind>Y</unwind>
    <unwind_map>rows</unwind_map>
    <returning_graph>N</returning_graph>
    <return_graph_field/>
    <mappings>
      <mapping>
        <parameter>productID</parameter>
        <field>ProductID</field>
        <type>Integer</type>
      </mapping>
      <mapping>
        <parameter>productName</parameter>
        <field>ProductName</field>
        <type>String</type>
      </mapping>
      <mapping>
        <parameter>supplierID</parameter>
        <field>SupplierID</field>
        <type>Integer</type>
      </mapping>
      <mapping>
