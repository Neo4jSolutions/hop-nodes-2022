          <constraint_type>UNIQUE</constraint_type>
          <object_name>Product</object_name>
          <object_properties>productID</object_properties>
          <object_type>NODE</object_type>
          <update_type>CREATE</update_type>
        </update>
        <update>
          <index_name>COU_SUPPLIER_SUPPLIERID</index_name>
          <constraint_type>UNIQUE</constraint_type>
          <object_name>Supplier</object_name>
          <object_properties>supplierID</object_properties>
          <object_type>NODE</object_type>
          <update_type>CREATE</update_type>
        </update>
        <update>
          <index_name>COU_EMPLOYEE_EMPLOYEEID</index_name>
          <constraint_type>UNIQUE</constraint_type>
          <object_name>Employee</object_name>
          <object_properties>employeeID</object_properties>
          <object_type>NODE</object_type>
          <update_type>CREATE</update_type>
        </update>
        <update>
          <index_name>COU_ORDERITEM_ORDERITEMID</index_name>
          <constraint_type>UNIQUE</constraint_type>
          <object_name>OrderItem</object_name>
          <object_properties>orderItemID</object_properties>
          <object_type>NODE</object_type>
          <update_type>CREATE</update_type>
        </update>
        <update>
          <index_name>COU_ORDER_ORDERID</index_name>
          <constraint_type>UNIQUE</constraint_type>
          <object_name>Order</object_name>
          <object_properties>orderID</object_properties>
          <object_type>NODE</object_type>
          <update_type>CREATE</update_type>
