      <description/>
      <type>NEO4J_CONSTRAINT</type>
      <attributes/>
      <connection>neo4j</connection>
      <updates>
        <update>
          <index_name>COU_CUSTOMER_CUSTOMERID</index_name>
          <constraint_type>UNIQUE</constraint_type>
          <object_name>Customer</object_name>
          <object_properties>customerID</object_properties>
          <object_type>NODE</object_type>
          <update_type>CREATE</update_type>
        </update>
        <update>
